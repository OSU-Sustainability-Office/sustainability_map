    "hjson": "^3.2.2",
    "leaflet": "^1.6.0",
    "leaflet-defaulticon-compatibility": "^0.1.2",
    "npm-run-all": "^4.1.5",
    "osmtogeojson": "^3.0.0-beta.4",
    "vue": "^2.6.10",
    "vue-i18n": "^8.17.3",
    "vue-router": "^3.1.6",
    "vue2-leaflet": "^2.5.2",
    "vuelayers": "^0.11.27",
    "vuex": "^3.2.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^1.6.1",
    "autoprefixer": "^9.8.8",
    "eslint": "^8.57.1",
    "eslint-config-standard": "^17.1.0",
