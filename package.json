    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-strict-vue": "^1.0.0",
    "eslint-plugin-vue": "^8.7.1",
    "prettier": "^2.7.1",
    "sass": "^1.82.0",
    "vite": "^6.0.4"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
