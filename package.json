{
  "name": "project_base",
  "version": "0.2.0",
  "private": true,
  "scripts": {
    "serve": "SET NODE_OPTIONS=--openssl-legacy-provider && vue-cli-service serve --open",
    "prebuild": "node util/build/*.js",
    "build": "vue-cli-service build",
    "build-stage": "node --max_old_space_size=4096 node_modules/.bin/vue-cli-service build --mode staging",
    "lint": "vue-cli-service lint",
    "prettier": "prettier --write **/*.{js,ts,tsx,json,html,md,css,scss,vue}",
    "format": "npm-run-all --continue-on-error prettier \"lint -- --fix\""
  },
  "dependencies": {
    "axios": "^1.7.9",
    "cache-loader": "^4.1.0",
    "element-ui": "^2.13.1",
    "hjson": "^3.2.2",
    "leaflet": "^1.6.0",
    "leaflet-defaulticon-compatibility": "^0.1.1",
    "osmtogeojson": "^3.0.0-beta.4",
    "prettier": "^2.7.1",
    "sass": "^1.26.10",
    "sass-loader": "^8.0.2",
    "vue": "^2.6.10",
    "vue-i18n": "^8.17.3",
    "vue-router": "^3.1.6",
    "vue-svg-loader": "^0.16.0",
    "vue2-leaflet": "^2.5.2",
    "vuelayers": "^0.11.27",
    "vuex": "^3.2.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.12.0",
    "@vue/cli-plugin-eslint": "^3.12.0",
    "@vue/cli-service": "^3.12.0",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-node": "^8.0.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-strict-vue": "^1.0.0",
    "eslint-plugin-vue": "^5.2.3",
    "npm-run-all": "^4.1.5",
    "style-resources-loader": "^1.3.3",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
